package com.fds.FoodDelivery.model;

import java.util.ArrayList;
import java.util.List;

public class Response {

		Integer code;
		String message;
		User  datas;
		ArrayList<User> data;

		public Response() {
		}

		public Response(Integer code, String message) {
			this.code = code;
			this.message = message;
		}


		public Integer getCode() {
			return code;
		}

		public void setCode(Integer code) {
			this.code = code;
		}

		public String getMessage() {
			return message;
		}

		public void setMessage(String message) {
			this.message = message;
		}

		public List<User> getUser() {
			return data;
		}

		public void setUser(List<User> data) {
			this.datas = datas;
		}
/*
		public User  getUser() {
			return data;
		}

		public void setUser(User data) {
			this.data = data;
		}
	*/	
		public Response(int code, String message, User data) {
			// TODO Auto-generated constructor stub
			this.code = code;
			this.message = message;
			this.datas = data;
			System.out.print(code);
			System.out.print(' '+message);
		}

		public Response(int code, String message, ArrayList<User> users) {
			// TODO Auto-generated constructor stub
			this.message = message;
			this.data = users;
			System.out.print(code);
			System.out.print(' '+message);
		}

		@Override
		public String toString() {
			return "Response [code=" + code + ", "+" message=" + message + "]";
		}

	}
